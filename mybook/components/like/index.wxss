    padding: 10rpx;
}

.container image{
    width: 32rpx;
    height: 28rpx;
}

.container text{
    font-size: 24rpx;
    line-height: 24rpx; /* 会消除文字的上下间距 */
    color: #bbb;
    position: relative;
    bottom: 10rpx;
    left: 6rpx;
}